<bpmn:userTask
  id="UserTask"
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:zeebe="http://camunda.org/schema/zeebe/1.0"
>
  <bpmn:extensionElements>
    <zeebe:taskListeners>
      <zeebe:taskListener eventType="complete" retries="3" type="complete_listener"/>
    </zeebe:taskListeners>
  </bpmn:extensionElements>
</bpmn:userTask>
